{
  "name": "@teages/nuxt-legacy",
  "type": "module",
  "version": "1.1.1",
  "packageManager": "pnpm@10.24.0",
  "description": "A Nuxt module for supporting legacy browsers.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Teages/nuxt-legacy.git"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "pnpm -C playgrounds/v4 dev",
    "dev:v3": "pnpm -C playgrounds/v3 dev",
    "dev:build": "pnpm -C playgrounds/v4 build",
    "dev:build-v3": "pnpm -C playgrounds/v3 build",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && pnpm --filter \"./playgrounds/*\" dev:prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "peerDependencies": {
    "@vitejs/plugin-legacy": "^7.0.0",
    "nuxt": "^3.18.0 || >=4.0.3"
  },
  "peerDependenciesMeta": {
    "@vitejs/plugin-legacy": {
      "optional": true
    }
  },
  "dependencies": {
    "@nuxt/kit": "catalog:",
    "browserslist": "catalog:"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@nuxt/devtools": "catalog:",
    "@nuxt/eslint-config": "catalog:",
    "@nuxt/module-builder": "catalog:",
    "@nuxt/schema": "catalog:",
    "@nuxt/test-utils": "catalog:",
    "@types/node": "catalog:",
    "@vitejs/plugin-legacy": "catalog:",
    "changelogen": "catalog:",
    "changelogithub": "catalog:",
    "eslint": "catalog:",
    "node-html-parser": "catalog:",
    "nuxt": "catalog:nuxt4",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "vue-tsc": "catalog:"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
